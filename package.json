{
  "name": "@jjordy/swr-devtools",
  "version": "0.2.22",
  "description": "Devtools for SWR",
  "private": false,
  "main": "./dist/index.js",
  "module": "./esm/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**",
    "esm/**"
  ],
  "scripts": {
    "build": "rimraf esm && rimraf dist && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --target ESNext --module ES6 --outDir esm",
    "build:cjs": "ncc build src/index.tsx -o dist -m -e react",
    "dev": "npm run storybook",
    "storybook": "start-storybook -p 9001",
    "watch": "tsc --watch"
  },
  "keywords": [
    "SWR",
    "Devtools",
    "Devtool"
  ],
  "author": "Jordan Addison <jordanrileyaddison@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@storybook/addon-info": "^5.3.14",
    "@storybook/react": "^5.3.14",
    "@types/node": "^13.9.0",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@zeit/ncc": "^0.21.1",
    "babel-loader": "^8.0.6",
    "react": "^16.13.0",
    "react-docgen-typescript-loader": "^3.7.0",
    "react-dom": "^16.13.0",
    "rimraf": "^3.0.2",
    "swr": "^0.2.0-beta.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "idb": "^5.0.1",
    "react-json-view": "^1.19.1",
    "react-rnd": "^10.1.6"
  },
  "peerDependencies": {
    "react": "^16.13.0",
    "swr": "^0.2.0-beta.0"
  }
}
