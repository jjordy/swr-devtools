{
  "name": "@jjordy/swr-devtools",
  "version": "2.1.2",
  "description": "Devtools for SWR",
  "private": false,
  "main": "./dist/index.js",
  "module": "./esm/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**",
    "esm/**"
  ],
  "scripts": {
    "build": "rimraf esm && rimraf dist && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --target ESNext --module ES6 --outDir esm",
    "build:cjs": "ncc build src/index.tsx -o dist -m -e react",
    "dev": "tsc --watch",
    "storybook": "start-storybook -p 9001",
    "test": "jest",
    "watch": "tsc --watch"
  },
  "keywords": [
    "SWR",
    "Devtools",
    "Devtool"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "roots": [
      "<rootDir>/src"
    ],
    "setupFilesAfterEnv": [
      "./setupTests.js"
    ]
  },
  "author": "Jordan Addison <jordanrileyaddison@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.1",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "@vercel/ncc": "^0.31.1",
    "babel-jest": "^27.3.1",
    "jest": "^27.3.1",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "swr": "^1.0.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "react-json-view": "^1.21.3",
    "react-rnd": "^10.3.5"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "swr": "^1.0.0"
  }
}
