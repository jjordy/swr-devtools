{
  "name": "@jjordy/swr-devtools",
  "version": "2.0.0",
  "description": "Devtools for SWR",
  "private": false,
  "main": "./dist/index.js",
  "module": "./esm/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**",
    "esm/**"
  ],
  "scripts": {
    "build": "rimraf esm && rimraf dist && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --target ESNext --module ES6 --outDir esm",
    "build:cjs": "ncc build src/index.tsx -o dist -m -e react",
    "dev": "npm run storybook",
    "storybook": "start-storybook -p 9001",
    "watch": "tsc --watch"
  },
  "keywords": [
    "SWR",
    "Devtools",
    "Devtool"
  ],
  "author": "Jordan Addison <jordanrileyaddison@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/react": "^6.3.7",
    "@types/node": "^16.7.9",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@vercel/ncc": "^0.30.0",
    "babel-loader": "^8.2.2",
    "react": "^17.0.2",
    "react-docgen-typescript-loader": "^3.7.0",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "swr": "^1.0.0",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "idb": "^6.1.3",
    "react-json-view": "^1.21.3",
    "react-rnd": "^10.3.4"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "swr": "^1.0.0"
  }
}
