{
  "name": "@jjordy/swr-devtools",
  "version": "1.0.1",
  "description": "Devtools for SWR",
  "private": false,
  "main": "./dist/index.js",
  "module": "./esm/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**",
    "esm/**"
  ],
  "scripts": {
    "build": "rimraf esm && rimraf dist && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --target ESNext --module ES6 --outDir esm",
    "build:cjs": "ncc build src/index.tsx -o dist -m -e react",
    "dev": "npm run storybook",
    "storybook": "start-storybook -p 9001",
    "watch": "tsc --watch"
  },
  "keywords": [
    "SWR",
    "Devtools",
    "Devtool"
  ],
  "author": "Jordan Addison <jordanrileyaddison@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/react": "^6.2.9",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "@vercel/ncc": "^0.28.3",
    "babel-loader": "^8.2.2",
    "react": "^17.0.2",
    "react-docgen-typescript-loader": "^3.7.0",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "swr": "^0.5.5",
    "ts-loader": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "idb": "^6.0.0",
    "react-json-view": "^1.21.3",
    "react-rnd": "^10.2.4"
  },
  "peerDependencies": {
    "react": "^16.13.0",
    "swr": "^0.2.0-beta.0"
  }
}
